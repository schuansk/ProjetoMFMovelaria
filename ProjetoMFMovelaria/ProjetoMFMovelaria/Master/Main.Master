                            </asp:HyperLink>
                        </li>
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown3" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                <i class="fas fa-cog"></i><span></span>
                                <asp:Label ID="lblName" CssClass="disabled" runat="server" Text=""> </asp:Label>
                            </a>
                            <div class="dropdown-menu" aria-labelledby="navbarDropdown3">
                                <asp:HyperLink ID="hlkExit" CssClass="dropdown-item" runat="server" NavigateUrl="~/Pages/EmployeePage/Exit.aspx" Text="Sair">
                                    <i class="fas fa-sign-out-alt"></i> Sair
                                </asp:HyperLink>
                            </div>
