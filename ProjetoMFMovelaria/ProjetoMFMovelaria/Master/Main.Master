<%@ Master Language="C#" AutoEventWireup="true" CodeBehind="Main.master.cs" Inherits="ProjetoMFMovelaria.Master.Main" %>

<!DOCTYPE html>

<html>
<head runat="server">
    <title></title>
    <%-- Arquivos CSS --%>
    <link href="../Content/bootstrap.min.css" rel="stylesheet" />
    <link href="../Content/DataTables/css/dataTables.bootstrap.min.css" rel="stylesheet" />
    <link href="../Content/DataTables/css/jquery.dataTables.min.css" rel="stylesheet" />
    <link href="../Content/DataTables/css/buttons.dataTables.min.css" rel="stylesheet" />
    <asp:ContentPlaceHolder ID="head" runat="server">
    </asp:ContentPlaceHolder>
</head>
<body>
    <form id="form1" runat="server">
        <div>
            <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
                <asp:HyperLink ID="HyperLink1" CssClass="navbar-brand text-white" runat="server" NavigateUrl="~/Pages/EmployeePage/Home.aspx">MF Movelaria</asp:HyperLink>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <div class="collapse navbar-collapse" id="navbarSupportedContent">
                    <ul class="navbar-nav mr-auto">
                        <%-- Dropdown itens que chamam as paginas --%>
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">Fornecedor
                            </a>
                            <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                                <asp:HyperLink ID="hlkRegisterSupplier" CssClass="dropdown-item " runat="server" Text="Cadastrar fornecedor" NavigateUrl="~/Pages/SupplierPages/Register.aspx">
                                    <i class="fas fa-plus-circle"></i> Cadastrar novo fornecedor
                                </asp:HyperLink>
                                <asp:HyperLink ID="hlkToListSupplier" CssClass="dropdown-item" runat="server" NavigateUrl="~/Pages/SupplierPages/ToList.aspx" Text="Listar fornecedores">
                                    <i class="fas fa-list-alt"></i> Listar fornecedores
                                </asp:HyperLink>
                            </div>
                        </li>
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown2" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">Orçamento
                            </a>
                            <div class="dropdown-menu" aria-labelledby="navbarDropdown2">
                                <asp:HyperLink ID="HyperLink4" CssClass="dropdown-item" runat="server" NavigateUrl="~/Pages/BudgetPages/Default.aspx" Text="Upload planilha">
                                    <i class="fas fa-list-alt"></i> Upload planilha
                                </asp:HyperLink>
                            
                                <asp:HyperLink ID="HyperLink2" CssClass="dropdown-item" runat="server" NavigateUrl="~/Pages/BudgetPages/ToListBudget.aspx" Text="Listar orçamentos">
                                    <i class="fas fa-list-alt"></i> Listar orçamentos
                                </asp:HyperLink>
                            </div>
                        </li>
                        <li class="nav-item">
                            <asp:HyperLink ID="HyperLink3" CssClass="nav-link" runat="server" NavigateUrl="~/Pages/EmployeePage/SendEmail.aspx" Text="Enviar pedido">
